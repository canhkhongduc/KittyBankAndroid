<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gamification.kitty.hackathon.kittybank.activity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BankActivity" />
        <activity android:name=".TransactActivity" />
        <activity android:name=".AccountDetailActivity" />
        <activity android:name=".TransactMoneyActivity" />
        <activity android:name=".StoreActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".HomeKittyActivity"></activity>
        <activity android:name=".ProfileActivity" />
        <activity android:name=".HybridActivity"></activity>
    </application>

</manifest>